<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Esprit\RealEstateAgencyBundle\Entity\Client" table="client">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="50" nullable="false"/>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
    <field name="nom" type="string" column="nom" length="50" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="true"/>
    <field name="datenaissance" type="date" column="dateNaissance" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="50" nullable="true"/>
    <field name="telephone" type="string" column="telephone" length="15" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="true"/>
  </entity>
</doctrine-mapping>
