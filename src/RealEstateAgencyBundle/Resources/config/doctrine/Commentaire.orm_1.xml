<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Esprit\RealEstateAgencyBundle\Entity\Commentaire" table="commentaire">
    <id name="idcommentaire" type="integer" column="idCommentaire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idoffre" type="integer" column="idOffre" nullable="false"/>
    <field name="idowner" type="integer" column="idOwner" nullable="false"/>
    <field name="dateajout" type="string" column="dateAjout" length="50" nullable="false"/>
    <field name="contenu" type="string" column="Contenu" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
